{
  "name": "feel good comments",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Replaces YouTube comments with the message You're Beautiful.",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*", 
        "*://youtube.com/*"
	  ],
      "js": ["feelgood.js"]
    }
  ]
}